{
  "name": "tardigrades/sexy-field-api",
  "description": "This package adds api functionality to sections.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Dion Snoeijen",
      "email": "hallo@dionsnoeijen.nl"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "symfony/http-foundation": "^5",
    "symfony/form": "^5",
    "symfony/config": "^5",
    "symfony/dependency-injection": "^5",
    "jms/serializer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Tardigrades\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tardigrades\\": "tests/unit"
    }
  },
  "suggest": {
    "tardigrades/sexy-field": "Without the base package this api is nothing",
    "tardigrated/sexy-field-entity": "You need entities, yes you do",
    "tardigrades/sexy-field-doctrine": "You might want to use doctrine for persisting data",
    "tardigrades/sexy-field-form": "The api requires you to use form"
  },
  "require-dev": {
    "tardigrades/sexy-field": "dev-sym5",
    "tardigrades/sexy-field-entity": "dev-sym5",
    "tardigrades/sexy-field-doctrine": "dev-sym5",
    "tardigrades/sexy-field-form": "dev-sym5",
    "tardigrades/sexy-field-field-types-base": "dev-sym5",
    "phpunit/phpunit": "^9",
    "mockery/mockery": "^1.0",
    "mikey179/vfsstream": "^1.6"
  }
}
